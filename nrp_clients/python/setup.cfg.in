[metadata]
name = nrp_client
version = @NRP_VERSION@
author = NRP Team
author_email = vorobev@in.tum.de
description = Python client for the NRP-Core
classifiers =
    Programming Language :: Python :: 3

[options]
python_requires = >=3.8
install_requires =
    psutil
    grpcio-tools >= 1.49.1
    nrp-protobuf == @NRP_VERSION@

[options.extras_require]
docker = 
    docker
    python-on-whales
    pyyaml
