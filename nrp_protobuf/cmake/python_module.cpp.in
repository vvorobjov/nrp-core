#include <boost/python.hpp>

#include "nrp_general_library/config/cmake_constants.h"
#include "nrp_general_library/datapack_interface/datapack.h"

#include "nrp_protobuf/proto_python_bindings/proto_python_bindings.h"
#include "nrp_protobuf/proto_python_bindings/repeated_field_proxy.h"

@PROTO_INCLUDE_STR@

/*!
 * \file python_module.cpp
 * \brief Protobuf msg python module
 * DO NOT EDIT THIS FILE DIRECTLY! The code is automatically generated via cmake
 */

BOOST_PYTHON_MODULE(@PROTO_PYTHON_MODULE_NAME@) {

    // Import General NRP Python Module
    boost::python::import(PYTHON_MODULE_NAME_STR);

    // Create bindings for Repeated scalar field helpers
    RepeatedScalarFieldProxy::create();
    RepeatedScalarFieldIterProxy::create();

    // Proto msg and related DataPack class python bindings goes below
@PROTO_BINDING_DEFS@

}
